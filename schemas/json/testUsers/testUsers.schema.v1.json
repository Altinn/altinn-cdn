{
    "$id": "https://altinncdn.no/schema/json/testUsers/testUsers.schema.v1.json",
    "type": "object",
    "properties": {
      "$schema": { "type": "string" },
      "persons": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "partyId": {
              "type": "integer"
            },
            "ssn": {
              "type": "string"
            },
            "firstName": {
              "type": "string"
            },
            "middleName": {
              "type": "string"
            },
            "lastName": {
              "type": "string"
            },
            "customClaims": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  },
                  "value": {
                    "type": "string"
                  },
                  "valueType": {
                    "type": "string"
                  }
                },
                "required": ["type", "value","valueType"],
                "additionalProperties": false
              }
            },
            "partyRoles": {
              "type": "object",
              "patternProperties": {
                "^\\d+$": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "type": {
                        "type": "string"
                      },
                      "value": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false,
                    "required": ["type", "value"]
                  }
                }
              },
              "additionalProperties":false
            },
            "addressCity": {
              "type": "string"
            },
            "addressHouseLetter": {
              "type": "string"
            },
            "addressHouseNumber": {
              "type": "string"
            },
            "addressMunicipalName": {
              "type": "string"
            },
            "addressMunicipalNumber": {
              "type": "string"
            },
            "addressPostalCode": {
              "type": "string"
            },
            "addressStreetName": {
              "type": "string"
            },
            "mailingAddress": {
              "type": "string"
            },
            "mailingPostalCity": {
              "type": "string"
            },
            "mailingPostalCode": {
              "type": "string"
            },
            "mobileNumber": {
              "type": "string"
            },
            "telephoneNumber": {
              "type": "string"
            },
            "email": {
              "type": "string"
            },
            "userId": {
              "type": "integer"
            },
            "userName": {
              "type": "string"
            },
            "language": {
              "type": "string"
            }
          },
          "required": ["partyId", "userId", "ssn", "partyRoles"],
          "additionalProperties": false
        }
      },
      "orgs": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "partyId": {
              "type": "integer"
            },
            "parentPartyId": {
              "type": "integer"
            },
            "orgNumber": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "businessAddress": {
              "type": "string"
            },
            "businessPostalCity": {
              "type": "string"
            },
            "businessPostalCode": {
              "type": "string"
            },
            "eMailAddress": {
              "type": "string"
            },
            "faxNumber": {
              "type": "string"
            },
            "internetAddress": {
              "type": "string"
            },
            "mailingAddress": {
              "type": "string"
            },
            "mailingPostalCity": {
              "type": "string"
            },
            "mailingPostalCode": {
              "type": "string"
            },
            "mobileNumber": {
              "type": "string"
            },
            "telephoneNumber": {
              "type": "string"
            },
            "unitStatus": {
              "type": "string"
            },
            "unitType": {
              "type": "string"
            }
          },
          "required": ["partyId", "orgNumber"],
          "additionalProperties": false
        }
      }
    },
    "required": ["Persons", "Orgs"],
    "additionalProperties": false
  }
  