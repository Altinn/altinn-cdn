{
    "$id": "https://altinncdn.no/schemas/json/test-users/test-users.schema.v1.json",
    "type": "object",
    "properties": {
      "$schema": { "type": "string" },
      "persons": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "partyId": {
              "type": "integer"
            },
            "partyUuid": {
              "type": "string",
              "format": "uuid",
              "description": "UUID of the party. Needs to be valid if the party is to be used for instance delegation (e.g. for parallel signing)"
            },
            "ssn": {
              "type": "string",
              "description": "Empty string is interpreted as self-identified"
            },
            "firstName": {
              "type": "string"
            },
            "middleName": {
              "type": "string"
            },
            "lastName": {
              "type": "string"
            },
            "customClaims": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "type": {
                    "type": "string"
                  },
                  "value": {
                    "type": "string"
                  },
                  "valueType": {
                    "type": "string"
                  }
                },
                "required": ["type", "value","valueType"],
                "additionalProperties": false
              }
            },
            "partyRoles": {
              "type": "object",
              "patternProperties": {
                "^\\d+$": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "type": {
                        "type": "string"
                      },
                      "value": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false,
                    "required": ["type", "value"]
                  }
                }
              },
              "additionalProperties":false
            },
            "addressCity": {
              "type": "string"
            },
            "addressHouseLetter": {
              "type": "string"
            },
            "addressHouseNumber": {
              "type": "string"
            },
            "addressMunicipalName": {
              "type": "string"
            },
            "addressMunicipalNumber": {
              "type": "string"
            },
            "addressPostalCode": {
              "type": "string"
            },
            "addressStreetName": {
              "type": "string"
            },
            "mailingAddress": {
              "type": "string"
            },
            "mailingPostalCity": {
              "type": "string"
            },
            "mailingPostalCode": {
              "type": "string"
            },
            "mobileNumber": {
              "type": "string"
            },
            "telephoneNumber": {
              "type": "string"
            },
            "email": {
              "type": "string"
            },
            "userId": {
              "type": "integer"
            },
            "userName": {
              "type": "string"
            },
            "language": {
              "type": "string"
            }
          },
          "required": ["partyId", "userId", "ssn", "partyRoles"],
          "additionalProperties": false
        }
      },
      "orgs": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "partyId": {
              "type": "integer"
            },
            "partyUuid": {
              "type": "string",
              "format": "uuid",
              "description": "UUID of the party. Needs to be valid if the party is to be used for instance delegation (e.g. for parallel signing)"
            },
            "parentPartyId": {
              "type": "integer"
            },
            "orgNumber": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "businessAddress": {
              "type": "string"
            },
            "businessPostalCity": {
              "type": "string"
            },
            "businessPostalCode": {
              "type": "string"
            },
            "eMailAddress": {
              "type": "string"
            },
            "faxNumber": {
              "type": "string"
            },
            "internetAddress": {
              "type": "string"
            },
            "mailingAddress": {
              "type": "string"
            },
            "mailingPostalCity": {
              "type": "string"
            },
            "mailingPostalCode": {
              "type": "string"
            },
            "mobileNumber": {
              "type": "string"
            },
            "telephoneNumber": {
              "type": "string"
            },
            "unitStatus": {
              "type": "string"
            },
            "unitType": {
              "type": "string"
            }
          },
          "required": ["partyId", "orgNumber"],
          "additionalProperties": false
        }
      },
      "systems": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "systemUsers": {
              "type": "array",
              "items": {
                "type": "object",
                "properties": {
                  "id": {
                    "type": "string",
                    "format": "uuid"
                  },
                  "orgNumber": {
                    "type": "string",
                    "description": "The organisation number of the system user, must refer to an organisation in the orgs array"
                  },
                  "actions": {
                    "type": "array",
                    "items": {
                      "type": "string"
                    },
                    "description": "List of actions that are delegated to the system users, e. g. ['read', 'write', 'instantiate']"
                  }
                },
                "required": ["id", "orgNumber", "actions"],
                "additionalProperties": false
              }
            }
          },
          "required": ["id", "name", "systemUsers"],
          "additionalProperties": false
        }
      }
    },
    "required": ["persons", "orgs", "systems"],
    "additionalProperties": false
}
